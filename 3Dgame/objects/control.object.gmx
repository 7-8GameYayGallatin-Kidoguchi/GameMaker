    if pausechoice == 1 {
        pause=0;
    }
    if pausechoice == 2 {
        //control menu
    }
    if pausechoice == 3 {
        game_end();
    }
    if o_player.hp == 0{
        if continues &gt;= 0{
            if pausechoice == 1 {
                o_player.hp = 50;
                pause=0;
                continues--;
            }
            if pausechoice == 2 {
                game_end();
            }
        }
        else
            game_end();
    }
}
